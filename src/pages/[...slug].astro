---
import Layout from '../layouts/Layout.astro';
const currentPath = Astro.url.pathname;
const canonicalURL = new URL(Astro?.url?.origin) + currentPath;
const slug = Astro.params;

console.log('slug', slug)
console.log('canonical', canonicalURL)


export function getStaticPaths() {

  return [
    {params: { slug: 'test' }}
  ];
}


// console.log(slug)
---

<Layout title={currentPath}>
	<main>
        <h1 class="text-white">The current URL is: {Astro.url}</h1>
        <h1 class="text-white">The current URL pathname is: {Astro.url.pathname}</h1>
        <h1 class="text-white">The current URL origin is: {Astro.url.origin}</h1>
    </main>
</Layout>  
